{
  "name": "change-checker",
  "version": "1.0.3",
  "description": "Utility for creating generic deep typed diffs to track changes between lifecycles of objects.",
  "keywords": [
    "change",
    "tracking",
    "checker",
    "diff",
    "difference",
    "deep",
    "type",
    "typed",
    "dirty",
    "changed",
    "deleted",
    "touched",
    "created",
    "create",
    "change",
    "delete",
    "tracker",
    "typescript"
  ],
  "types": "./types/Index.d.ts",
  "main": "./dist/es5-minified/change-checker.minified.js",
  "module": "./dist/es5/Index.js",
  "es2015": "./dist/es6/Index.js",
  "unpkg": "./dist/es5-minified/change-checker.minified.js",
  "scripts": {
    "clean": "rimraf ./dist/**/* && rimraf ./types/**",
    "tslint": "tslint -c tslint.json -t verbose --force 'src/**/*.ts'",
    "tslint-fix": "tslint -c tslint.json -t verbose --force 'src/**/*.ts' --fix",
    "build": "yarn run clean && node ./node_modules/typescript/bin/tsc --project tsconfig.es5.json && node ./node_modules/typescript/bin/tsc --project tsconfig.es6.json && webpack --config build/webpack.config.js",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "testperf": "mocha -r ts-node/register test/**/*.spec-perf.ts",
    "postinstall": "code -r CHANGELOG.md"
  },
  "repository": "https://github.com/HohensteinGroup/ChangeChecker.git",
  "author": "Maxim Brunnmeier <m.brunnmeier@hohenstein.de>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "rimraf": "~2.6.2",
    "ts-loader": "~5.3.0",
    "tslint": "~5.11.0",
    "tslint-loader": "~3.6.0",
    "typescript": "~3.1.5",
    "webpack": "~4.23.1",
    "webpack-cli": "~3.1.2",
    "webpack-merge": "~4.1.4",
    "webpack-node-externals": "~1.7.2",
    "whatwg-fetch": "~3.0.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^8.0.2"
  }
}
